[program:{{ gunicorn.app.name }}]
command = "{{ gunicorn.config.executable }}"
user = {{ gunicorn.config.user }}
stdout_logfile = {{ gunicorn.log.file }}
redirect_stderr = true
stdout_logfile_maxbytes=100MB

[inet_http_server]
port = 127.0.0.1:9001
username = supervisor
password = supervisor@123

[supervisorctl]
serverurl = unix://{{ supervisor.runtime.socket }}
username = chris
password = 123

[supervisord]
logfile = {{ supervisor.log.file }}
logfile_maxbytes = 50MB
logfile_backups=10
loglevel = info
pidfile = {{ supervisor.runtime.pidfile }}
nodaemon = false
minfds = 1024
minprocs = 200
umask = 022
user = {{ supervisor.config.user }}
identifier = supervisor
directory = /tmp
nocleanup = true
childlogdir = /tmp
strip_ansi = false